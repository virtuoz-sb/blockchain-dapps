{
  "private": true,
  "version": "0.1.0",
  "name": "@torobot/api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "set NODE_ENV=development&& node -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "dev": "set NODE_ENV=development&& nodemon",
    "debug": "set NODE_ENV=development&& nest start --debug --watch",
    "serve ": "set NODE_ENV=production&& node dist/main",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "web-push-keys": "web-push generate-vapid-keys",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker": "npm run docker:build; npm run docker:up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:stop": "pm2 stop torobot-api",
    "pm2:start": "set NODE_ENV=production&& pm2 start dist/src/main.js --name=torobot-api",
    "pm2:restart": "npm run pm2:stop && npm run pm2:start"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@types/coinbase": "^2.0.8",
    "ccxt": "^1.73.85",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.6.15",
    "@nestjs/swagger": "^4.5.7",
    "@nestjs/websockets": "^7.6.15",
    "@types/speakeasy": "^2.0.7",
    "apple-signin-auth": "^1.5.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "code-config": "^1.0.37",
    "facebook-auth-nestjs": "^0.0.3",
    "firebase-admin": "^9.11.0",
    "googleapis": "^82.0.0",
    "handlebars": "^4.7.7",
    "nest-winston": "^1.6.2",
    "nodemailer": "^6.6.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io": "^4.1.3",
    "socket.io-redis": "^6.1.1",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^4.2.0",
    "web-push": "^3.4.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.31",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.8",
    "@types/web-push": "^3.3.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
