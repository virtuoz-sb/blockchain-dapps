{
  "name": "api",
  "version": "0.6.4",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "beforecompilation": "rimraf dist && npm run mkdirs && npm run copyprotojs",
    "beforecompilation:win": "node run_windows.js",
    "prebuild": "npm run beforecompilation",
    "build": "npm run prebuild && nest build",
    "build:tsc": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:devdocker": "npm rebuild && npm i && npm run start:dev",
    "start:dev": "npm run beforecompilation && nest start --watch",
    "start:dev:win": "npm run beforecompilation:win && nest start --watch",
    "start:debug": "npm run beforecompilation && nest start --debug --watch",
    "start:prod": "npm run beforecompilation && node dist/main",
    "lint:ts": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:prettier": "prettier --check \"**/*.{md,mdx,html,json,yml,ts,tsx,css,scss}\"",
    "lint": "npm run lint:ts && npm run lint:prettier",
    "test:watch": "jest --watch",
    "test:watch:e2e": "jest --watch --config ./test/jest-e2e.json ",
    "test:e2e": "jest --clearCache && jest --config ./test/jest-e2e.json --runInBand",
    "test": "jest --clearCache && jest --config ./test/jest-all.json --runInBand --verbose false",
    "test:unit": "jest --clearCache && jest --runInBand --verbose false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "proto:gen": "./scripts/generate-proto.sh",
    "mkdirs": "mkdir -p dist/src/proto",
    "copyprotojs": "cp -r src/proto dist/src && find dist/src/proto -type f -name '*.ts' -delete",
    "grpc:hello": "ts-node -r tsconfig-paths/register test/grpc/hello-grpc-server.ts"
  },
